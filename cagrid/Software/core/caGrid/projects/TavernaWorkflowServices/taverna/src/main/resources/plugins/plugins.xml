            <repository>http://ftp.cica.es/mirrors/maven2/</repository>
        </repositories>
        <profile>
            <dependency>
                <groupId>org.cagrid.transfer</groupId>
                <artifactId>cagridtransfer-activity</artifactId>
                <version>1.1</version>
            </dependency>
