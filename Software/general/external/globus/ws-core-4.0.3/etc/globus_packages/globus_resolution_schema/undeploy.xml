<project default="undeploy" basedir=".">


<property environment="env"/>
<property file="build.properties"/>
<property file="${user.home}/build.properties"/>
<property name="env.GLOBUS_LOCATION" value="../../.."/>
<property name="deploy.dir" value="${env.GLOBUS_LOCATION}"/>


<target name="undeploy">
  <delete verbose="true" dir="${deploy.dir}/etc/globus_resolution_schema"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/resolutionService/naming.xsd"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/resolutionService/resolution_bindings.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/resolutionService/resolution_port_type.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/resolutionService/resolution_service.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/resolutionService/test/test_counter_bindings.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/resolutionService/test/test_counter_port_type.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/resolutionService/test/test_counter_service.wsdl"/>
</target>
</project>
