  <delete verbose="true" file="${deploy.dir}/share/schema/transferService/transfer_bindings.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/transferService/transfer_port_type.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/transferService/transfer_service.wsdl"/>
</target>
</project>
