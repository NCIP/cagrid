<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	<class name="org.cagrid.identifiers.namingauthority.domain.IdentifierValues">
 		<map-to xml="IdentifierValues"
        	ns-uri="http://na.cagrid.org/1.0/NamingAuthority"
         	ns-prefix="na" />
		<field name="values" collection="map">
			<bind-xml name="KeyValues">
				<class name="org.exolab.castor.mapping.MapItem">
				    <map-to xml="KeyValues"
                        ns-uri="http://na.cagrid.org/1.0/NamingAuthority"
                        ns-prefix="na" />
					<field name="key" type="java.lang.String">
						<bind-xml name="key" node="element" />
					</field>
					<field name="value" collection="arraylist" type="java.lang.String">
						<bind-xml name="value" />
					</field>
				</class>
			</bind-xml>
		</field>
	</class>
	
	<class name="org.cagrid.identifiers.namingauthority.domain.NamingAuthorityConfig">
		<map-to xml="Configuration"
			ns-uri="http://na.cagrid.org/1.0/NamingAuthority"
         	ns-prefix="na" />
		<field name="gridSvcUrl" type="java.lang.String">
			<bind-xml name="gridSvcUrl"/>
		</field>
	</class>
</mapping>
